[Unit]
Description=Gunicorn server for SITENAME;

[Service]
Restart=on-failure
User=billynote
WorkingDirectory=/home/billynote/sites/SITENAME/source
ExecStart=/home/billynote/sites/SITENAME/venv/bin/gunicorn \
    --bind unix:/tmp/SITENAME.socket \
    superlists.wsgi:application

[Install]
WantedBy=multi-user.target
